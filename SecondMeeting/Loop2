public class Loop2 {
   public static void main(String[] args) {
        int i = 0;
        int j = 7;
        
        while (i < 3) {
            while (j < 10) {
                System.out.print("Apple");
                j++ ;
            }
            	System.out.println("PIE");
            	i++ ;
            	j -= 3;
        }
    }
}
